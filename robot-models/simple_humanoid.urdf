<?xml version="1.0" ?>

<robot name="simple_humanoid" xmlns:xacro="http://ros.org/wiki/xacro">

    <link name="base"/>

    <link name="torso">
        <inertial>
            <mass value="5.0"/>
            <origin xyz="0.0 0.0 0.0"/>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
        </inertial>
    </link>
    <joint name="torso_joint" type="floating">
        <parent link="base"/>
        <child link="torso"/>
        <origin xyz="0.0 0.0 0.0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="right_leg">
        <inertial>
            <mass value="0.25"/>
            <origin xyz="0.0 0.0 0.0"/>
            <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.05" iyz="0" izz="0.05"/>
        </inertial>
    </link>
    <joint name="right_leg_joint" type="revolute">
        <parent link="torso"/>
        <child link="right_leg"/>
        <origin xyz="0.0 -0.1 -0.4"/>
        <axis xyz="0 1 0"/>
        <limit effort="150" lower="-10" upper="10" velocity="100"/>
    </joint>

    <link name="left_leg">
        <inertial>
            <mass value="0.25"/>
            <origin xyz="0.0 0.0 0.0"/>
            <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.05" iyz="0" izz="0.05"/>
        </inertial>
    </link>
    <joint name="left_leg_joint" type="revolute">
        <parent link="torso"/>
        <child link="left_leg"/>
        <origin xyz="0.0 0.1 -0.4"/>
        <axis xyz="0 1 0"/>
        <limit effort="150" lower="-10" upper="10" velocity="100"/>
    </joint>

    <link name="right_arm">
        <inertial>
            <mass value="0.1"/>
            <origin xyz="0.0 0.0 0.0"/>
            <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.02"/>
        </inertial>
    </link>
    <joint name="right_arm_joint" type="revolute">
        <parent link="torso"/>
        <child link="right_arm"/>
        <origin xyz="0.0 -0.1 0.2"/>
        <axis xyz="0 1 0"/>
        <limit effort="150" lower="-10" upper="10" velocity="100"/>
    </joint>

    <link name="left_arm">
        <inertial>
            <mass value="0.1"/>
            <origin xyz="0.0 0.0 0.0"/>
            <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.02"/>
        </inertial>
    </link>
    <joint name="left_arm_joint" type="revolute">
        <parent link="torso"/>
        <child link="left_arm"/>
        <origin xyz="0.0 0.1 0.2"/>
        <axis xyz="0 1 0"/>
        <limit effort="150" lower="-10" upper="10" velocity="100"/>
    </joint>

</robot>
