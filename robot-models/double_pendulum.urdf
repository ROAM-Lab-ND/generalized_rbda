<?xml version="1.0" ?>

<robot name="double_pendulum" xmlns:xacro="http://ros.org/wiki/xacro">

    <link name="base_link"/>

    <link name="link1">
        <inertial>
            <mass value="2.0"/>
            <origin xyz="0.0 0.0 0.0"/>
            <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.03" iyz="0" izz="0.01"/>
        </inertial>
    </link>

    <link name="link2">
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0.0 0.0 0.0"/>
            <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.2"/>
        </inertial>
    </link>

    <joint name="joint1" type="continuous">
        <parent link="base_link"/>
        <child link="link1"/>
        <origin xyz="0.0 0.0 0.0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <joint name="joint2" type="continuous">
        <parent link="link1"/>
        <child link="link2"/>
        <origin xyz="0.1 0.0 0.0"/>
        <axis xyz="0 0 1"/>
    </joint>

</robot>
